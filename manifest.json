{
  "manifest_version": 3,
  "name": "Tab Organizer",
  "version": "1.0",
  "description": "Displays all open tabs.",
  "permissions": ["tabs", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "open_tab_viewer": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      },
      "description": "Open Tab Organizer"
    }
  }
}
